<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/css/style.css">
    <script src="https://kit.fontawesome.com/071897bdcc.js" crossorigin="anonymous"></script>
    <link href="https://fonts.googleapis.com/css?family=Overpass" rel="stylesheet">
    <title>Ã–zel Bismil Teknoloji Koleji</title>
</head>
<body>
    <%- include("./parts/header.ejs") %>
      <div class="container index">
            <h1 class="duyuru-haberler">Elektrik ve Elektronik</h1>
            <hr>
            <div class="row row-cols-1 row-cols-md-3 g-4">
                <% bolumler.filter((el) => el.category === "Elektrik ve Elektronik").forEach(x => { %>
                    <a href="<%= x.img %>" target="_blank" class="link-dark">
                        <div class="col bolumlerimiz">
                            <img width="400px" src="<%= x.img %>">   
                            
                    <center><h3><%= x.isim %></h3></center>
                        </div> 
                    </a>
                <% }) %>
          </div>
      </div>
      <div class="container index">
        <h1 class="duyuru-haberler">Yenilenebilir Enerji</h1>
        <hr>
        <div class="row row-cols-1 row-cols-md-3 g-4">
            <% bolumler.filter((el) => el.category === "Yenilenebilir Enerji").forEach(x => { %>
                <a href="<%= x.img %>" target="_blank" class="link-dark">
                    <div class="col bolumlerimiz">
                        <img width="400px" src="<%= x.img %>">
                        
                <center><h3><%= x.isim %></h3></center>
                    </div> 
                </a>
            <% }) %>
      </div>
      <%- include("./parts/footer.ejs") %>
</body>
</html>